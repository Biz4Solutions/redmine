<% if @member.errors.empty? %>
  $('#tab-content-members').html('<%= escape_javascript(render :partial => 'projects/settings/members') %>');
  hideOnLoad();
  $("#member-<%= @member.id %>").effect("highlight");
<% else %>
  alert('<%= raw(escape_javascript(@member.errors.full_messages.join(", "))) %>');
<% end %>
